{
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        },
        {
            "src": "templates/index.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/questions",
            "dest": "app.py"
        },
        {
            "src": "/check",
            "dest": "app.py"
        },
        {
            "src": "/(.*)",
            "dest": "templates/index.html"
        }
    ]
}